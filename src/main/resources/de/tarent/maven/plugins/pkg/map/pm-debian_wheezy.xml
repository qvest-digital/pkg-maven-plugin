            <!-- JGoodies Looks -->
            <entry>
                <artifactSpec>jgoodies:looks</artifactSpec>
                <dependencyLine>libjgoodies-looks-java</dependencyLine>
                <jars>
                    <jar>looks.jar</jar>
                </jars>
