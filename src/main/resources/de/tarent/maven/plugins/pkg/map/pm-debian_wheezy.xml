<!-- Package maps document for Debian GNU/Linux Wheezy. -->
<package-maps>
    <version>1.0</version>

    <distro>
        <id>debian_wheezy</id>
        <label>Debian GNU/Linux Wheezy</label>
        <packaging>deb</packaging>
        <repoName>wheezy</repoName>
        <defaultJarPath>/usr/share/java</defaultJarPath>
        <!-- Altough Debian puts most of its JNI libraries into /usr/lib/jni
            some end up in /usr/lib (e.g. librxtx-java). So we add this path as well. -->
        <defaultJNIPath>/usr/lib/jni:/usr/lib</defaultJNIPath>
        <defaultDependencyLine>default-jre</defaultDependencyLine>

        <map>
            <entry>
                <artifactSpec>net.java.dev.jna:jna</artifactSpec>
                <versionSpec>[3,3.2.7]</versionSpec>
                <dependencyLine>libjna-java</dependencyLine>
                <jars>
                    <jar>jna.jar</jar>
                </jars>
            </entry>

            <!-- Maven knows multiple SWT variants -->
            <entry>
                <artifactSpec>org.eclipse.swt:swt-gtk-linux
                </artifactSpec>
                <dependencyLine>libswt-gtk-3-java</dependencyLine>
                <jars>
                    <jar>swt.jar</jar>
                </jars>
            </entry>

            <!-- The following 2 entries are found in the JBoss repositories -->
            <entry>
                <artifactSpec>org.eclipse.swt:org.eclipse.swt.gtk.linux.x86
                </artifactSpec>
                <dependencyLine>libswt-gtk-3-java</dependencyLine>
                <jars>
                    <jar>swt.jar</jar>
                </jars>
            </entry>

            <entry>
                <artifactSpec>org.eclipse.swt:org.eclipse.swt.gtk.linux.x86_64
                </artifactSpec>
                <dependencyLine>libswt-gtk-3-java</dependencyLine>
                <jars>
                    <jar>swt.jar</jar>
                </jars>
            </entry>

            <entry>
                <artifactSpec>asm:asm</artifactSpec>
                <dependencyLine>libasm3-java</dependencyLine>
                <jars>
                    <jar>asm3.jar</jar>
                </jars>
            </entry>

            <entry>
                <artifactSpec>asm:asm-util</artifactSpec>
                <dependencyLine>libasm3-java</dependencyLine>
                <jars>
                    <jar>asm3-util.jar</jar>
                </jars>
            </entry>

            <entry>
                <artifactSpec>asm:asm-commons</artifactSpec>
                <dependencyLine>libasm3-java</dependencyLine>
                <jars>
                    <jar>asm3-commons.jar</jar>
                </jars>
