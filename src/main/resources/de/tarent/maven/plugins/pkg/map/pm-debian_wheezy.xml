            </entry>

            <!-- Everything can be automatically derived from the artifactSpec -->
            <entry>
                <artifactSpec>junit:junit</artifactSpec>
                <versionSpec>[3,4)</versionSpec>
            </entry>

            <entry>
                <artifactSpec>junit:junit</artifactSpec>
                <versionSpec>[4,4.10]</versionSpec>
                <dependencyLine>junit4</dependencyLine>
                <jars>
                    <jar>junit4.jar</jar>
                </jars>
            </entry>

            <entry>
